#include <iostream>

using namespace std;

int main()
{
    int n,m,j,i;
    cin>>n>>m;
    int M[n+1][m+1];
     for(i=1;i<=n;i++)
     {
        for(j=1;j<=m;j++)
        {
        if(i<=j)
         M[i][j]=i;
         else M[i][j]=j;
        }
     }
    for(i=1;i<=n;i++)
     {
        for(j=1;j<=m;j++)
        cout<<M[i][j]<<" ";
        cout<<endl;
     }
    return 0;
}
